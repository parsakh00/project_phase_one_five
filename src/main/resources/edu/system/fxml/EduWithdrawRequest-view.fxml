<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.system.gui.EduWithdrawRequest">
   <children>
      <Pane prefHeight="125.0" prefWidth="800.0" styleClass="loginBox" stylesheets="@../styles.css">
         <children>
            <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnBtnClicked" stylesheets="@../styles.css" text="&lt;-" />
            <Label alignment="CENTER" layoutX="260.0" layoutY="18.0" prefHeight="17.0" prefWidth="280.0" text="withdraw from education requests">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Label fx:id="source" layoutX="14.0" layoutY="136.0" prefHeight="18.0" prefWidth="322.0" text="Student's requests for withdrawal from education requests :" />
      <TextField fx:id="choiceUser" layoutX="337.0" layoutY="133.0" promptText="choose from choice box" />
      <ChoiceBox fx:id="chooseUser" layoutX="337.0" layoutY="159.0" prefWidth="150.0" />
      <Button layoutX="526.0" layoutY="133.0" mnemonicParsing="false" onAction="#rejection" text="Reject" />
      <Button layoutX="597.0" layoutY="133.0" mnemonicParsing="false" onAction="#accept" text="Accept" />
   </children>
</AnchorPane>
